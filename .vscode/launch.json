{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Run Server",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "disney_customers_feedback_ex.main:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/env_disney_customers_feedback_ex/bin/python"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "python": "${workspaceFolder}/env_disney_customers_feedback_ex/bin/python"
        }
    ]
}
